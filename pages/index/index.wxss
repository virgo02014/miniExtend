/**index.wxss**/
.header {
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  margin: 0 32rpx;
  border-bottom: 1px dashed #ddd;
}

.title {
  font-size: 50rpx;
}

.subTitle {
  font-size: 26rpx;
  color: #c0c0c0;
}

.avatar {
  width: 100rpx;
  height: 100rpx;
  margin: 50rpx 0 30rpx;
  border-radius: 50%;
}

.content > view {
  padding: 10rpx 32rpx;
  margin: 10rpx 0;
  font-size: 32rpx;
}
.content {
  color: #555;
}
.weui-cell_active {
  color: #333;
  background-color: #eee;
}

.previewimg{
    float:left;
    width:45%;
    height:200px;
    margin-right: 2%;
}
.previewimg image{
    width:100%;
    height:100%;
    border: 1px solid #eee;
}
